  "scripts": {
    "dev": "rm -rf src/__tests__ && next dev",
    "build": "rm -rf src/__tests__ && next build",
    "build:analyze": "ANALYZE=true next build",
    "analyze:client": "ANALYZE=true npm run build && echo 'Client bundle analysis complete. Check .next/analyze/client.html'",
    "analyze:server": "ANALYZE_SERVER=true npm run build && echo 'Server bundle analysis complete. Check .next/analyze/server.html'",
    "analyze:both": "ANALYZE=true ANALYZE_SERVER=true npm run build && echo 'Bundle analysis complete. Check .next/analyze/ folder'",
    "analyze:report": "node scripts/analyze-bundle.js",
    "analyze:full": "node scripts/analyze-bundle.js --build",
    "build:profile": "next build --profile",
    "start": "next start",
    "lint": "next lint",
    "test": "jest --config jest.config.js",
    "test:watch": "jest --config jest.config.js --watch",
    "test:coverage": "jest --config jest.config.js --coverage",
    "test:splits": "jest --config jest.config.js soapboxSplits",
    "deps:analyze": "npx depcheck",
    "deps:unused": "npx depcheck --json | jq '.dependencies + .devDependencies | keys[]'",
